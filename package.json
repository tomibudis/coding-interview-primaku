{
  "name": "@virtual-spirit/codelabs-next-web",
  "version": "1.0.1",
  "description": "This boilerplate is a starter nextjs app to building web platform",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtualspirit/codelabs-next-web.git"
  },
  "keywords": [
    "boilerplate",
    "next",
    "app"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/virtualspirit/codelabs-next-web/issues"
  },
  "homepage": "https://github.com/virtualspirit/codelabs-next-web#readme",
  "bin": {
    "codelabs-next-web": "bin/cli.js"
  },
  "author": "tomibudis",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "istanbul-badges-readme",
    "extract:colors": "node -r esm scripts/extract-sass-vars.js",
    "check:types": "npx tsc --noEmit",
    "postinstall": "npx husky install",
    "lighthouse": "npx lighthouse-badges --urls http://localhost:3000 -o lighthouse"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.22.0",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "esm": "^3.2.25",
    "lodash": "^4.17.21",
    "next": "^12.1.5",
    "next-pwa": "^5.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.49.7",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@tanstack/react-query-devtools": "^4.22.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "17.0.13",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.45.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "cssnano": "^5.1.14",
    "eslint": "8.8.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "husky": "^7.0.4",
    "istanbul-badges-readme": "^1.8.1",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.19",
    "prettier": "^2.5.1",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.5.5",
    "webpack": "^5.75.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "main": "readme.md"
}
